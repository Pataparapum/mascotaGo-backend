{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mascotasgo-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mascotasgo-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mascotasgo-app",
  "projects": {
    "mascotasgo-app": {
      "type": "application",
      "root": "apps/mascotasgo-app",
      "entryFile": "main",
      "sourceRoot": "apps/mascotasgo-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mascotasgo-app/tsconfig.app.json"
      }
    },
    "user-crud": {
      "type": "application",
      "root": "apps/user-crud",
      "entryFile": "main",
      "sourceRoot": "apps/user-crud/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-crud/tsconfig.app.json"
      }
    }
  }
}